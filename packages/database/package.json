{
  "name": "@deskops/database",
  "version": "1.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "bun run prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts",
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@deskops/constants": "workspace:*",
    "@prisma/client": "^6.18.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@deskops/eslint-config": "workspace:*",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "prisma": "^6.18.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.3"
  }
}
